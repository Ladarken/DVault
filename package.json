{
  "name": "dvault",
  "version": "1.0.0",
  "description": "DarkVault",
  "main": "truffle-config.js",
  "scripts": {
    "test": "npx truffle test",
    "compile": "npx truffle compile",
    "deploy": "npx oz deploy",
    "start": "npx ganache-cli -l 10000038 -i 1234 -e 100000 -a 10 -u 0 -g 1000000000 --allowUnlimitedContractSize -m \"$HDWALLET_MNEMONIC\"",
    "version": "npx truffle version",
    "extract-abis": "npx truffle-extract -o .abis",
    "solhint": "npx solhint -f table contracts/**/*.sol",
    "prettier:solidity": "npx prettier --write contracts/**/*.sol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ladarken/dVault.git"
  },
  "author": "Ladarken",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ladarken/dVault/issues"
  },
  "homepage": "https://github.com/Ladarken/dVault#readme",
  "dependencies": {
    "@openzeppelin/cli": "^2.8.2",
    "@truffle/hdwallet-provider": "^1.1.0",
    "dotenv": "^8.2.0",
    "eth-gas-reporter": "^0.2.17",
    "ganache-cli": "^6.11.0",
    "mocha-junit-reporter": "^2.0.0",
    "prettier": "^2.1.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.58",
    "solc": "^0.7.2",
    "solhint": "^3.2.1",
    "truffle": "^5.1.46",
    "truffle-assertions": "^0.9.2",
    "truffle-extract": "^1.2.1"
  }
}
